    chaa_chakma jaa_chakma jhaa_chakma nyaa_chakma ttaa_chakma tthaa_chakma ddaa_chakma
    ddhaa_chakma daa_chakma dhaa_chakma paa_chakma phaa_chakma baa_chakma yyaa_chakma
    yaa_chakma waa_chakma ];

    @_impacted_group_2_gsub_8_chained = [ kaa_chakma.below ghaa_chakma.below ttaa_chakma.below tthaa_chakma.below nnaa_chakma.below taa_chakma.below thaa_chakma.below
    naa_chakma.below bhaa_chakma.below maa_chakma.below laa_chakma.below saa_chakma.below haa_chakma.below ];

    @_lookahead_group_3_gsub_8_chained = [ uMatra_chakma oMark_chakma uMatra_chakma.alt ];
    @_lookahead_group_4_gsub_8_chained = [ aMatra_chakma maayyaa_chakma ];

    sub @_backtrack_group_2_gsub_8_chained @_impacted_group_2_gsub_8_chained' lookup gsub_2_single;

    } gsub_8_chained ;

