lookup gsub_5_chained {
    sub [ u_chakma e_chakma khaa_chakma gaa_chakma ngaa_chakma caa_chakma chaa_chakma jaa_chakma jhaa_chakma nyaa_chakma ddhaa_chakma daa_chakma dhaa_chakma paa_chakma phaa_chakma baa_chakma maa_chakma yyaa_chakma yaa_chakma waa_chakma  ] [ raa_chakma.below raa.below_oMark  ]' lookup gsub_3_single ;
    subtable;
    sub [ gaa_chakma nnaa_chakma dhaa_chakma naa_chakma  ] [ uMatra_chakma  ]' lookup gsub_4_single ;

    } gsub_5_chained ;

lookup gsub_8_chained {
